# Tmux
function tns {
    tmux new-session -d -s $1 && [[ -n "$TMUX" ]] \
        && tmux switch-client -t $1 \
        || tmux attach
}
# Docker
#docker remove all
function dra {
    docker rm $(docker ps -aq)
}
#docker stop all
function dsa {
    docker stop $(docker ps -aq)
}
#docker prune all
function dpa {
    echo stop containers..
    dsa
    echo remove containers
    dra
    echo prune networks..
    docker network prune -f
    echo prune volumes..
    docker volume prune -f
}

