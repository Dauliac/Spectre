#!/bin/sh

# Clean trash
trash-empty 10 && \
echo [OK] $(date) Trash empted || \
echo [ERR] $(date) Trash not empted

# Backup FreeTube
\cp -f .config/FreeTube/settings.db Dotfiles/dotfiles/config/freetube/; \
\cp -f .config/FreeTube/subscriptions.db Dotfiles/dotfiles/config/freetube/ &&\
echo [OK] $(date) FreeTube db backuped || \
echo [ERR] $(date) FreeTube db not backuped


